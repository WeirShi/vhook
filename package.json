{
  "name": "vhook",
  "version": "1.0.0",
  "description": "Vue Composition Function",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "author": "lmhcoding",
  "private": false,
  "sideEffects": false,
  "files": [
    "lib/",
    "esm/"
  ],
  "keywords": [
    "vue3",
    "composition api"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https//github.com/lmhcoding/vhook"
  },
  "homepage": "https://github.com/lmhcoding/vhook#readme",
  "bugs": {
    "url": "https://github.com/lmhcoding/vhook/issues"
  },
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "cm": "cz",
    "lint": "eslint {src,tests}/**/*.ts",
    "lint:fix": "npm run lint --fix",
    "lint:types": "tsc --noEmit",
    "lint:prettier": "prettier --write src/**/*.ts",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "npm-run-all build:*",
    "clean": "rimraf lib",
    "release": "semantic-release",
    "test": "jest",
    "docs": "npm-run-all -p register docs:dev",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:deploy": "gh-pages -d ./docs/.vitepress/dist",
    "register": "gulp"
  },
  "peerDependencies": {
    "vue": "^3.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.6",
    "@types/jest": "^26.0.14",
    "@types/lodash.throttle": "^4.1.6",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "@vue/compiler-sfc": "^3.0.3",
    "@vue/test-utils": "^2.0.0-beta.11",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^3.1.0",
    "globby": "^11.0.1",
    "gulp": "^4.0.2",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.3.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.upperfirst": "^4.3.1",
    "npm-run-all": "^4.1.5",
    "ora": "^5.1.0",
    "prettier": "^2.1.2",
    "require-self": "^0.2.3",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.1.1",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "4.1.2",
    "vitepress": "0.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "throttle-debounce": "^2.3.0",
    "tslib": "^2.0.1",
    "vue": "^3.0.3"
  }
}
